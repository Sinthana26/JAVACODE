import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);  // Scanner object creation
        
        // Fixing nextInt() syntax
        int a = sc.nextInt();  
        int b = sc.nextInt();  
        int c = sc.nextInt();  

        // Finding the largest number using the conditional operator
        int big = (a > b ? (a > c ? a : c) : (b > c ? b : c));

        // Printing the largest number
        System.out.println("The largest number is: " + big);
        
        sc.close();  // Closing the scanner (Best practice)
    }
}
