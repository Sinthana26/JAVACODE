import java.util.*;
public class findMissedNumber {
    public static void main(String[] args) {
        Scanner io = new Scanner(System.in);
        int num = io.nextInt();
        int arr[] = new int[num];
        int sum = 0;
        for(int i = 0; i < num; i++)
        {
            arr[i] = io.nextInt();
            sum+=arr[i];
        }
        int natural = ((num+1)*(num+2))/2;
        System.out.println(natural-sum);
    }
}
