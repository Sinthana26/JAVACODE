import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner sc =new Scanner(System.in);
	System.out.println("Enter the number:");
    int num=sc.nextInt();
    int i=1;
    for(;;){
        if(i<11){
        System.out.println(num+ "x" +i +"="+ num*i);
        i++;
    }
  }
}
}
